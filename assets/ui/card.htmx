{{range .}}
<div class="col-4">
    <div class="card text-white {{ .Style }} border">
        <div class="card-body">
            <h5 class="card-title">{{ .Title }}</h5>
            <p class="card-text">{{ .Desc }}</p>
            <p class="card-text" id="card-{{ .ID }}"><small class="text-muted">ID: {{ .ID }}</small></p>
        </div>
    </div>
</div>
{{end}}